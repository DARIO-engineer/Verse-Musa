    "splash": {
      "image": "./assets/verse.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A237E"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/verse.png",
        "backgroundColor": "#1A237E"
      },
      "package": "com.darpimen.Verse",
      "buildType": "apk",
      "gradleCommand": ":app:assembleRelease",
      "enableAapt2": true,
      "enableProguardInReleaseBuilds": true,
      "enableSeparateBuildPerCPUArchitecture": true,
      "universalApk": false,
      "enableShrinkResourcesInReleaseBuilds": true,
      "enableDexMergingInReleaseBuilds": true,
      "enableR8": true,
      "versionCode": 1,
      "splits": {
        "abi": true,
        "density": false
